{{#with data}}
  <img src="{{this.image}}" alt="" />
  <h2>{{this.title}}</h2>
  <h3>{{this.director}}</h3>
  <p>Stars</p>
  <ul>
    {{#each this.stars}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <p>{{this.description}}</p>
  {{#each this.showtimes}}
    <p>{{this}}|</p>
  {{/each}}
{{/with}}