<div class="card-group">
  {{#each data}}
    <div class="card">
      <img src="{{this.image}}" class="card-img-top" alt="{{this.title}}" />
      <div class="card-body">
        <h5 class="card-title"> {{this.title}}</h5>
      </div>
      <div class="card-footer">
        <a href="/{{this._id}}"> See more</a>
      </div>
    </div>
  {{/each}}
</div>